{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".svelte-kit/**"]
		},
		"lint": {},
		"format": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	},
	"globalEnv": ["NODE_ENV", "CI"]
}
